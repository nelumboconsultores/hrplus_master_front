{
  "name": "hrplus_master_front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "qa": "vite --mode qa",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\" && eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "invalidation:dev": "aws cloudfront create-invalidation --distribution-id E4Z50R5KE938E --paths \"/*\"",
    "build:dev": "tsc && vite build --mode development",
    "upload:dev": "aws s3 sync dist/ s3://capital-humano-ui",
    "deploy:dev": "yarn format && yarn build:dev && yarn upload:dev && yarn invalidation:dev",
    "invalidation:qa": "aws cloudfront create-invalidation --distribution-id E2SRUN146G296U --paths \"/*\"",
    "build:qa": "tsc && vite build --mode qa",
    "upload:qa": "aws s3 sync dist/ s3://capital-humano-ui-qa",
    "deploy:qa": "yarn format && yarn build:qa && yarn upload:qa && yarn invalidation:qa"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@hello-pangea/dnd": "^16.5.0",
    "@hookform/resolvers": "3.3.3",
    "@mui/icons-material": "5.14.19",
    "@mui/lab": "5.0.0-alpha.158",
    "@mui/material": "5.15.2",
    "@mui/x-data-grid": "6.18.3",
    "@mui/x-date-pickers": "6.18.4",
    "@react-google-maps/api": "^2.19.3",
    "@types/crypto-js": "4.2.1",
    "@types/react-big-calendar": "^1.8.9",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.10",
    "i18next": "23.7.11",
    "i18next-browser-languagedetector": "7.2.0",
    "react": "18.2.0",
    "react-big-calendar": "^1.12.2",
    "react-code-input": "3.10.1",
    "react-colorful": "^5.6.1",
    "react-currency-mask": "^1.3.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-i18next": "14.0.0",
    "react-number-format": "5.2.2",
    "react-organizational-chart": "^2.2.1",
    "react-router-dom": "6.18.0",
    "react-sticky-el": "^2.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vitejs/plugin-react": "4.2.0",
    "eslint": "8.53.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.4",
    "prettier": "3.1.0",
    "typescript": "5.2.2",
    "vite": "5.0.0"
  }
}
